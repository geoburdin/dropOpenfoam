FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases          (lava air);

lava
{
    transportModel  Newtonian;
    nu              500;    // lava's kinematic viscosity
    rho             2500;    // lava's density
}

air
{
    transportModel  Newtonian;
    nu              1.48e-05;
    rho             1;
}

sigma            0.1;


